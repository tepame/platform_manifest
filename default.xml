<?xml version="1.0" encoding="UTF-8"?>
<manifest>

	<!-- remote origin definition -->
	<remote name="tepame"
		fetch="https://github.com/tepame"
		revision="master" />

	<!-- default origin source -->
	<default revision="master"
		 remote="tepame"
		 sync-c="true"
		 sync-j="4" />

	<!-- projects -->
	
	<!-- devices -->
	<project path="device/zte/blade_v580" name="device_zte_blade_v580" groups="pdk,devices" />
	<project path="device/zte/blade_v6" name="device_zte_blade_v6" groups="pdk,devices" />
	
	<!-- kernel -->
	<project path="kernel/zte/blade_v580" name="kernel_zte_blade_v580" groups="pdk,kernel" />
	<project path="kernel/zte/blade_v6" name="kernel_zte_blade_v6" groups="pdk,kernel" />
	
	<!-- vendor -->
	<project path="vendor/zte/blade_v580" name="vendor_zte_blade_v580" groups="pdf,vendor" />
	<project path="vendor/zte/blade_v6" name="vendor_zte_blade_v6" groups="pdk,vendor" />

</manifest>
