<?xml version="1.0" encoding="UTF-8"?>
<manifest>

	<!-- remote origin definition -->
	<remote name="tepame"
		fetch="https://github.com/tepame"
		revision="master" />

	<!-- default origin source -->
	<default revision="master"
		 remote="tepame"
		 sync-c="true"
		 sync-j="4" />

	<!-- devices -->
	<project path="device/zte/v580" name="zte_blade_v580" groups="pdk,devices" />
	<project path="device/zte/v6" name="zte_blade_v6" groups="pdk,devices" />

</manifest>
